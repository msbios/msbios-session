{
  "name": "msbios/session",
  "description": "MSBios Session",
  "license": "MIT",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-session",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Session\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Session\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-session": "^2.8",
    "msbios/mongodb": "^1.0"
  },
  "require-dev": {
    "msbios/application": "^1.0",
    "msbios/test": "^1.0",
    "msbios/debug": "^1.0",
    "san/san-session-toolbar": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "serve": "php -S 0.0.0.0:3107 -t public index.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push",
    "mongodb-start-osx": "brew services start mongodb",
    "mongodb-restart-osx": "brew services restart mongodb"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\Session"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.zendframework.com"
    }
  ]
}
